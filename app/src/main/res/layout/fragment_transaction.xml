<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
    <include
        android:id="@+id/title_transaction"
        layout="@layout/toolbar_title"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/title_transaction">
        <com.tencent.smtt.sdk.WebView
            android:id="@+id/wv_transaction"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></com.tencent.smtt.sdk.WebView>
    </RelativeLayout>
    <include
        android:id="@+id/progress_transaction"
        layout="@layout/progress_wheel"/>
</RelativeLayout>